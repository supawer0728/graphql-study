type User {
    id: Int!
    name(toUpperCase: Boolean): String!
    email: String!
    address: Address!
    phone: String!
    website: String!
    company: Company!
}

type Address {
    street: String!
    suite: String!
    city: String!
    zipcode: String!
    geo: GeoLocation!
}

type GeoLocation {
    lat: String!
    lng: String!
}

type Company {
    name: String!
    catchPhrase: String!
    bs: String!
}